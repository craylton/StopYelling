{
    "manifest_version": 2,
    "name": "StopYelling",
    "version": "1.0",

    "description": "Converts obnoxious UPPERCASE stream titles to sentence-case.",

    "icons": {
        "48": "icons/48.png"
    },

    "content_scripts": [
        {
            "matches": ["*://*.twitch.tv/*"],
            "js": ["stopYelling.js"]
        }
    ]
}
