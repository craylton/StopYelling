{
    "manifest_version": 3,
    "name": "StopYelling",
    "version": "1.4.0",
    "description": "Converts obnoxious UPPERCASE stream titles to sentence-case.",
    "icons": {
        "48": "icons/48.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "{c9fccb44-2bae-4fc9-a355-452871919bd4}",
            "strict_min_version": "109.0"
        }
    },
    "content_scripts": [
        {
            "matches": ["*://*.twitch.tv/*", "*://*.youtube.com/*"],
            "js": ["domObserverUtil.js", "stopYelling.js", "twitch.js", "youtube.js"]
        }
    ]
}